{
    "name": "location-keymap",
    "displayName": "location-keymap",
    "description": "",
    "version": "0.0.1",
    "publisher": "suizhiyuan",
    "repository": "https://github.com/sui-zhiyuan/location-keymap",
    "engines": {
        "vscode": "^1.59.0"
    },
    "main": "./out/extension.js",
    "categories": [
        "Keymaps"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands":[
            {
                "title": "Hello World",
                "command": "location-keymap.helloWorld"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+k",
                "command": "cursorUp",
                "when": "textInputFocus"
            },
            {
                "key": "alt+k",
                "command": "cursorUpBlock",
                "when": "textInputFocus"
            },
            {
                "key": "ctrl+h",
                "command": "cursorLeft",
                "when": "textInputFocus"
            },
            {
                "key": "alt+h",
                "command": "cursorWordLeft",
                "when": "textInputFocus"

            },
            {
                "key": "ctrl+j",
                "command": "cursorDown",
                "when": "textInputFocus"
            },
            {
                "key": "alt+j",
                "command": "cursorDownBlock",
                "when": "textInputFocus"
            },
            {
                "key": "ctrl+l",
                "command": "cursorRight",
                "when": "textInputFocus"
            },
            {
                "key": "alt+l",
                "command": "cursorWordRight",
                "when": "textInputFocus"
            }
        ]
    },
    "scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}